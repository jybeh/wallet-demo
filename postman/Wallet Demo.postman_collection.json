		{
			"name": "Get Transactions",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var uuid = require('uuid');",
							"var myUUID = uuid.v4();",
							"console.log(myUUID)",
							"pm.environment.set('idempotency_key', '\"' + myUUID + '\"')"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"accountID\": \"12345678\",\n    \"nextToken\": \"\",\n    \"limit\": 3\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "http://localhost:8080/v1/accounts/transactions/query",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8080",
					"path": [
